    @RenderSection("style", false)

    <title>@ViewData["Title"]</title>
</head>


<body>


    <!-- start header -->
    <header>
        <div style="height:51px;" class="header-first-part">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6 col-sm-12">
                        <div class="mail-order">
                            <ul>
                                <li style="margin-right:10px;">
                                    <i class="fas fa-envelope"></i>
                                    organico@gmail.com
                                </li>
                                <li>
                                    Free Shipping for all Order of $99
                                </li>
                            </ul>
                        </div>

                    </div>
                    <div class="col-lg-6 col-sm-12">
                        <div class="right-first-header">
                            <div class="exchange-login-social">
                                <ul style="display:flex;gap:18px;">

                                    @if (!User.Identity.IsAuthenticated)
                                    {
                                        <li class="login">

                                            <div class="login-icon">
                                                <i class="fas fa-user"></i>
                                                <span><a asp-action="SignUp" asp-controller="Account">Register</a></span>
                                            </div>


                                        </li>

                                        <li class="login">
                                            <div class="login-icon">
                                                <i class="fas fa-user"></i>
                                                <span><a asp-action="Login" asp-controller="Account">Login</a></span>
                                            </div>
                                        </li>
                                    }
                                    else
                                    {
                                        <span style="padding-top: 3px;">@User.Identity.Name</span>

                                        @if (User.IsInRole("Admin") || User.IsInRole("SuperAdmin"))
                                        {
                                            <a asp-action="Index" asp-controller="Dashboard" asp-area="AdminPanel"><i style="font-size:21px;color:black;" class="bi bi-person-square"></i></a>
                                        }

                                        <a asp-action="Logout" asp-controller="Account"><i style="font-size:22px;color:black;" class="bi bi-box-arrow-right"></i></a>


                                    }

                                </ul>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>

        <div class="header-second-part">
            <div class="container">
                <div class="row">
                    <div class="col-lg-3 col-sm-12">
                        <div class="ogani">
                            <a asp-action="HomePage" asp-controller="Home">
                                <img style="width:150px; "src="~/MemberPanel/assets/img/logo-organico.png" alt="ogani">
                            </a>


                        </div>
                    </div>
                    <div class="col-lg-6 col-sm-12">
                        <ul>
                            <li>
                                <a asp-action="HomePage" asp-controller="Home" id="drop">HOME</a>
                            </li>
                            <li>
                                <a asp-action="ProductPage" asp-controller="Product" id="drop">PRODUCT</a>
                            </li>
                            <li>
                                <a asp-action="ShopPage" asp-controller="Product" id="drop">SHOP</a>
                            </li>
                            <li>
                                <a asp-action="BlogPage" asp-controller="Blog" id="drop">BLOG</a>
                            </li>
                            <li>
                                <a asp-action="ContactPage" asp-controller="Home" id="drop">CONTACT</a>
                            </li>

                        </ul>
                    </div>
                    <div class="col-lg-3 col-sm-12">
                        <div class="basket-heart-price">

                            <span class="heart">
                                <a asp-action="FavoriteList" asp-controller="Home">
                                    <i class="fas fa-heart"></i>
                                </a>
                            </span>


                            <span class="basket">
                                <a asp-action="BasketList" asp-controller="Home">
                                    <i class="fas fa-shopping-basket"></i>
                                </a>
                            </span>

                            

                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="header-third-part">
            <div class="container">
                <div class="row">
                    <div class="col-lg-3 col-sm-12">
                        <div class="dropdown">
                            <button class="dropbtns">
                                <i class="fas fa-bars"></i>
                                <span>All Categories</span>
                                <i class="fas fa-angle-down"></i>
                            </button>
                            <div id="myDropdown" class="dropdown-content">
                                @await Component.InvokeAsync("CategoryList")
                            </div>
                        </div>

                    </div>
                    <div class="col-lg-6 col-sm-12">
                        <div class="search-input-category-btn d-flex">
                            <div class="dropdown-cate">
                                <button id="all-categ">All Categories <i class="fas fa-angle-down"></i></button>
                                <div class="dropdown-content-cate">
                                    @await Component.InvokeAsync("CategoryList")

                                </div>
                            </div>

                            @await Component.InvokeAsync("ProductSearch")

                        </div>

                    </div>
                    <div class="col-lg-3 col-sm-12">
                        <div class="phone-icon">
                            <div class="icon">
                                <i class="fas fa-phone-alt"></i>
                            </div>

                            <div class="phone">
                                <div class="number">
                                    +994 55 856 23 93
                                </div>
                                <div class="support">
                                    support 24/7 time
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </header>
    <!-- End-header -->


    <!-- Start CONTAINER -->

    @RenderBody()

    <!-- Start CONTAINER -->



    <!-- start-footer -->
    <footer>
        <div class="footer">
            <div class="container">
                <div class="adress-link-subs">
                    <div class="row">
                        <div class="col-lg-4 col-sm-12">
                            <img style="width:150px; " src="~/MemberPanel/assets/img/logo-organico.png" alt="ogani">
                            <div class="adress-phone-mail">
                                <p>
                                    Address: Semed Vurgun Street 110
                                </p>
                                <p>
                                    Phone: +994 55 856 23 93
                                </p>
                                <p>
                                    Email: organico@gmail.com
                                </p>
                            </div>
                        </div>
                        <div class="col-lg-4 col-sm-12">
                            <p class="useful-link">Useful Links</p>
                            <div class="links">

                                <div class="navbar">
                                    <a asp-action="HomePage" asp-controller="Home">Home</a>
                                    <a asp-action="ShopPage" asp-controller="Shop">Shop</a>
                                    <a asp-action="BlogPage" asp-controller="Blog">Blog</a>
                                    <a asp-action="ContactPage" asp-controller="Contact">Contact</a>

                                </div>
                                <div class="ourselves-link">
                                    <a href="https://en.wikipedia.org/wiki/Planet_Organic">Who We Are</a>
                                    <a href="">Our Services</a>
                                    <a href="">Projects</a>
                                    <a href="">Innovation</a>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-4 col-md-12">
                            <div class="footer__widget">
                                <h6>Join Our Social Media</h6>

                                <div class="footer__widget__social">
                                    <a href="#"><i class="fa fa-facebook"></i></a>
                                    <a href="#"><i class="fa fa-instagram"></i></a>
                                    <a href="#"><i class="fa fa-twitter"></i></a>
                                    <a href="#"><i class="fa fa-pinterest"></i></a>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>


            </div>
        </div>
    </footer>
    <!-- end-footer -->




